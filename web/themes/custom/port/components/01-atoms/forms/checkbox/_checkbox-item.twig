<li class="form-item--checkbox__item">
  <label for="checkbox{{ key }}">
    <input id="checkbox{{ key }}" class="form-item--checkbox__input" name="checkbox" type="checkbox" {% if checkbox_item_checked %} checked="{{ checkbox_item_checked }}" {% endif %}>
    <span class="form-item__label option"> {{ checkbox_item }} </span>
  </label>
</li>

